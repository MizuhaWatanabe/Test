/*---base---*/
* { margin:0px; padding:0px;}
body { font-family: "メイリオ","Meiryo",verdana,"ＭＳ Ｐゴシック","ヒラギノ角ゴ Pro W3","Hiragino Kaku Gothic Pro",Osaka,"MS PGothic",Sans-Serif; line-height: 1.5; font-size: small; text-align: center;}

body { color:#424242; background: #f6f2ed;}

h1,h2,h3,h4,h5,h6 { font-size:100%; font-weight: normal;} img { border:0; vertical-align:bottom;} li { list-style:none;} table { border:none; border-collapse:collapse; font-size:100%;} table caption { display:none;} td,th { text-align:left; line-height: 1.6;}

.mac body { font-family: "ヒラギノ角ゴ Pro W3","Hiragino Kaku Gothic Pro",Osaka,"メイリオ","Meiryo";}
.ie6 body { font-size:92%;}
.ie6 body ,.ff3 body { font-family: "ＭＳ Ｐゴシック";}

a:hover img.overwhite { filter: alpha(opacity=60); -ms-filter: "alpha(opacity=60)"; -moz-opacity:0.6; opacity:0.6; zoom:1; }

.shadow { box-shadow: 0px 0px 18px 3px #999999; border-radius: 10px; -webkit-border-radius: 10px; -moz-border-radius: 10px; }


/*--- frame layout ---*/
#wrapper             { width:950px; margin:0px auto; font-size: 105%;_overflow:hidden;*overflow:hidden!important; }
#container           { width:950px; margin:auto; margin-bottom:50px; }


/*---header---*/
div#header      { position:relative; width:950px; height: 200px; margin: 0 0 26px; text-align:center; _overflow:hidden; *overflow:hidden!important;}
div#header h2   { position: absolute; top: 38px; left: 41px; width: 299px; height: 65px; background: url(images/title.png) no-repeat; text-indent: -99999px; z-index: 10;}
div#header h2 a { display:block; width: 299px; height: 65px; overflow:hidden;}
div#header h3   { font-size: 14px; }
div#header p    { position: absolute; bottom: 34px; right: 78px; width: 220px; height: 73px; background: url(images/header_tel.png) no-repeat; text-indent: -99999px; z-index: 10;}
div#header p a  { display: block; width: 220px; height: 73px;}
div#header p.indexImg    { position: absolute; top: 20px; left: 306px; width: 230px; height: 220px; background: none; text-indent: 0; z-index: 9;}
div#header p.indexImg a  { display: block; width: 230px; height: 220px;}


/*---header list---*/
.listBtnsL      { float: left; width: 976px; padding: 20px 0 30px; margin: 0 0 0 -26px; _display: inline;}
.listBtnsL li   { float: left; width: 169px; margin: 0 0 0 26px; padding: 0 0 26px; _display: inline;}
.listBtnsL li a { cursor: pointer;}


/*--- mainBox layout ---*/
#mainBox             { width:730px; margin:auto; background: url(); text-align:center; margin-bottom: 50px; overflow:hidden; overflow:hidden !important; }


/*---update---*/
#update { height: 300px; width: 600px; margin-bottom: 54px; border: solid 1px #676767; overflow: auto; margin:auto; padding: 10px; }
#update td { vertical-align:top; }
.news { padding-left:10px; padding-bottom:10px; }


/*--- form layout ---*/
#mainBox.form        { width:600px; background: #cccccc; background: #dfdfdf; padding: 2em; margin: auto; text-align: right; -moz-border-radius: 8px; -webkit-border-radius: 8px; border-radius: 8px; zoom: 1; box-shadow: 0px 0px 18px 3px #999999; border-radius: 10px; -webkit-border-radius: 10px; -moz-border-radius: 10px; overflow:hidden;}
#mainBox.form table  { margin:auto; }
#mainBox.form h2     { font-size: 20px }
#mainBox.form h3     { text-align:left; font-size: 11px;}
#mainBox.form th     { background-color: #A2AAAA; text-align: left; padding: 8px; white-space: nowrap; color: #FFFFFF; border-top-width: 1px; border-right-width: 4px; border-bottom-width: 1px; border-left-width: 1px; border-top-style: solid; border-right-style: solid; border-bottom-style: solid; border-left-style: solid; border-top-color: #C9C9BE; border-right-color: #D7D7CE; border-bottom-color: #C9C9BE; border-left-color: #C9C9BE; font-size: 13px; font-family: Arial, Helvetica, sans-serif; }
#mainBox.form td     { width:400px; background-color: #F7F7F4; text-align: left; padding: 8px; border: 1px solid #D6D6CD; font-size: 13px; line-height: 20px; }
#mainBox.label       { vertical-align:top;}


/*--- table layout ---*/
#mainBox.table       { width:640px; margin:auto; overflow:hidden;}
#mainBox.table th    { width:200px; background-color: #A2AAAA; text-align: right; padding: 8px; white-space: nowrap; color: #FFFFFF; border-top-width: 1px; border-right-width: 4px; border-bottom-width: 1px; border-left-width: 1px; border-top-style: solid; border-right-style: solid; border-bottom-style: solid; border-left-style: solid; border-top-color: #C9C9BE; border-right-color: #D7D7CE; border-bottom-color: #C9C9BE; border-left-color: #C9C9BE; font-size: 13px; font-family: Arial, Helvetica, sans-serif; }
#mainBox.table td    { width:400px; background-color: #F7F7F4; text-align: left; padding: 8px; border: 1px solid #D6D6CD; font-size: 13px; line-height: 20px; }


/*--- IE6対応 ---*/
.ie6 #mainBox             { position: relative; z-index: 10;}
.ie6 #mainBoxBottom       { position: relative; z-index: 20;}
.ie6 #mainBoxContents     { position: relative; z-index: 30;}
.ie6 #main                { position: relative; z-index: 100; margin: -34px 26px 0 0;}


h1 { height: 40px; line-height: 40px; text-align: left; font-size:71%;}
.mac.ff3 h1 { font-size:73.5%;} .win.ie6 h1 { font-size:66%;}


/*---menuline layout---*/
#menuline                       { float: left; width: 220px; margin:auto 0px auto 0px; color: #333333; text-align:left; font-size: 15px; padding-bottom: 20px; }
#menuline_box                   { width:200px; margin: 10px 0px 10px 0px; padding:0px 0px 0px 0px; color: #333333; }
#menuline_box h2                { color: #003366; font-weight:normal; font-size:160%; margin:20px 0px 10px 0px; padding:0px; height:auto; border:none; background:none; }
#menuline_box ul                { width:220px; margin:0px 0px 0px 0px; padding:0px 0px 0px 0px; list-style:none; border-top: #000000 dotted 1px; }
#menuline_box li                { width:200px; margin:0px; padding:10px; border-bottom: #000000 dotted 1px; }
#menuline_box li a              { width:125px; padding:7px 20px 7px 20px; background-repeat:no-repeat; text-decoration:none; background-position:5px 9px; display:block; }
#menuline_box li a:link         { color: #003366; text-decoration:none; background-image:url(../images/list_bg.gif); }
#menuline_box li a:visited      { color: #003366; text-decoration:none; background-image:url(../images/list_bg.gif); }
#menuline_box li a:hover        { color: #003366; text-decoration:none; background-image:url(../images/list_hover_bg.gif); text-decoration:underline; }
#menuline_box li a.stay         { color: #660066; text-decoration:none; background-image:url(../images/list_hover_bg.gif); }
#menuline_box img               { margin:20px 20px 0px 20px; border:#999999 solid 1px; }


/*---footer---*/
#footerWrapper { float:left; width: 100%; clear:both; background: url('lp/footer_bg.jpg') 0px 0px repeat-x; text-align: center; color: #6a6a6a;}
#footer { position: relative; margin:0px auto; width:950px; height: 289px; text-align:left; line-height: 1.5;}

.toTop { position: absolute; top: 0px; right: 0px;}

#footer ul.fNav01    { position: absolute; top: 10px; right: 110px; height: 12px;}
#footer ul.fNav01 li { float: left; margin: 0 0 0 11px; line-height:1; _display:inline;}

#footer dl    { position: absolute; top: 49px;}
#footer dl dt { padding: 0 0 25px;}

#footer .fTxt01 { position: absolute; top: 50px; right: 0px;}

#footer dl.fSitemap { position: absolute; top: 208px; left: 0; width: 100%; padding: 20px 0 0; background: url(images/footer_border.gif) repeat-x; font-size: 71%;}
#footer dl.fSitemap dt { padding: 0;}
#footer dl.fSitemap dd { padding: 17px 0 0;}
#footer dl.fSitemap li { display: inline; line-height: 1.8;}

.designBy { position: absolute; bottom: 54px; right: 365px; display: block; width: 221px; height: 12px; line-height:12px; background: url(images/footer_web_design.png) no-repeat;}
.designBy a { display: block; width: 221px; height: 12px; text-indent: -99999px;}
address   { position: absolute; bottom: 54px; right: 0; display: block; width: 257px; height: 12px; line-height:12px; background: url(images/footer_copy.png) no-repeat; text-indent: -99999px;}

#footer a:link,#footer a:visited { color:#6a6a6a; text-decoration:none;}
#footer a:active,#footer a:hover { color:#FFF; text-decoration:none;}

#footer a:link strong,#footer a:visited strong { color:#e23543; text-decoration:none;}


